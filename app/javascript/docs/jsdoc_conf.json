{
    "opts": {
        "destination": "./app/javascript/docs/doc/",
        "p": true,
        "recurse": true
    },
    "plugins": ["plugins/markdown", "../jsdoc-vuejs"],
    "recurseDepth": 10,
    "source": {
        "include": ["./app/javascript"],
        "exclude": ["./app/javascript/docs/scripts"],
        "includePattern": ".+\\.(js|vue)(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false
    }
}
