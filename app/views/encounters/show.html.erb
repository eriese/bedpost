<div class="dashes hidden to-hide"></div>
<div class="center">
  <%= @user.name %> with <%= @partner.name %><br>

  Date: <%= @encounter.took_place %><br>
  Self-assessed risk level:
  <%= @encounter.self_risk %><br>
  App-assessed risk level:
  <%# if @encounter.risk <= 4 %>
    <!-- <span class="low">low</span> -->
  <%# elsif @encounter.risk <= 8 %>
    <!-- <span class="medium">medium</span> -->
  <%# else %>
    <!-- <span class="high">high</span> -->
  <%# end %><br>
  Notes: <%= @encounter.notes %><br>
  <br>
  <% @contacts.each do |contact| %>
  <% if !@user.genital_name.empty? && contact[:user_inst] == "genitals"%>
    <%= "Your #{@user.genital_name} came in contact with " %>
  <% elsif !@user.anus_name.empty? && contact[:user_inst] == "anus" %>
        <%= "Your #{@user.anus_name} came in contact with" %>
  <% else %>
    <%= "Your #{contact[:user_inst]} came in contact with " %>
  <% end %>
    <%= @pronoun[:possessive] %>
  <% if !@partner.genital_name.empty? && contact[:partner_inst] == "genitals" %>
    <%= @partner.genital_name %>
  <% elsif !@partner.anus_name.empty? && contact[:partner_inst] == "anus" %>
    <%= @partner.anus_name %>
  <% else %>
    <%= contact[:partner_inst] %>
  <% end %>
  <% if contact[:barriers] %>
    <%= "with a barrier" %><br>
  <% else %>
    <%= "without a barrier" %><br>
  <% end %>
  <% end %>
  Fluid exchange: <%= @encounter.fluid %> <br>
  <br><%= link_to "edit", edit_encounter_path(@encounter)%></div>
