<%= vuelidate_form_with model: encounter, url: [partnership, encounter], html: {class: "centered"}, value_include: [:contacts] do |f| %>
	<%= f.date_field :took_place %>
	<%= f.text_area :notes %>
	<%= f.range_field :self_risk, in: Diagnosis::TransmissionRisk::NO_RISK..Diagnosis::TransmissionRisk::HIGH %>
	<dynamic-field-list component-type="encounter-contact-field" :list="vf.formData.contacts" base-name="<%= f.object_name %>[contacts_attributes]" @should-validate="<%= f.class::SLOT_SCOPE %>.addValidation" track="barriers"></dynamic-field-list>
	<%= f.radio_group :fluids, options: {radio_class: "hidden-radio hidden-radio--cta", group_options: {field_class: "toggler", tooltip: true}} %>
	<%= f.submit :class => "cta" %>
<% end %>
