<% trans_key = @is_partner ? '.with_partner' : '.all' %>
<h2 class="centered-text"><%= t(".title.#{trans_key}", partner_name: @partnerships[0]["display"]) %></h2>
<div class="desc"><%= link_to(t(".all"), encounters_path) if @is_partner %></div>
<encounter-calendar>
	<h3><%= t(".no_encounters.#{trans_key}", partner_name: @partnerships[0]["partner_name"]) %></h3>
</encounter-calendar>
<div class="edit-button"><%= link_to t(".new"), @partnerships.size == 1 ? new_partnership_encounter_path(@partnerships[0]["_id"]) : encounters_who_path %></div>
